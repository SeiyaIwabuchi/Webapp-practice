package status;
import java.util.Random;

public class Status {
	private static Random random = new Random();
	
	public String getResult(){
		int n = random.nextInt(10);
		if(n<2){
			return "‘å‹g";
		}
		else if(n<5){
			return "‹g";
		}
		else if(n<7){
			return "‹¥";
		}
		else{
			return "‘å‹¥";
		}
		
	}
}
